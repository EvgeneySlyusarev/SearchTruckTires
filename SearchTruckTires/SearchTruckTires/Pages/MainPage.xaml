<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:SearchTruckTires.Pages" x:DataType="local:MainPage"
            x:Class="SearchTruckTires.Pages.MainPage"
            SelectedTabColor="Yellow"
            UnselectedTabColor="Red"
            BarBackgroundColor="DarkSlateGray"
            BackgroundColor="DarkSlateGray">
    <TabbedPage.Children>
        <local:AddUsedTires></local:AddUsedTires>
        <local:FindUsedTires></local:FindUsedTires>
        <local:TiresForShipment></local:TiresForShipment>
        <local:LoginPages></local:LoginPages>
        <local:RegistrationPage></local:RegistrationPage>
    </TabbedPage.Children>
</TabbedPage>


